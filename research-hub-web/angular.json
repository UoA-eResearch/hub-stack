{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "research-hub-web": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "outputPath": "www",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "tsconfig.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "./src/assets"
            ],
            "styles": [
              "src/styles.scss",
              "src/app/components/error-routing/error-routing.scss"
            ],
            "scripts": [],
            "allowedCommonJsDependencies": [
              "graphql-tag",
              "crypto-js",
              "zen-observable"
            ]
          },
          "configurations": {
            "local": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6mb"
                }
              ]
            },
            "prod": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6mb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "nonprod": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6mb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.nonprod.ts"
                }
              ]
            },
            "sandbox": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6mb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.sandbox.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "research-hub-web:build"
          },
          "configurations": {
            "local": {
              "browserTarget": "research-hub-web:build:local"
            },
            "sandbox": {
              "browserTarget": "research-hub-web:build:sandbox"
            },
            "nonprod": {
              "browserTarget": "research-hub-web:build:nonprod"
            },
            "prod": {
              "browserTarget": "research-hub-web:build:prod"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "research-hub-web:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "codeCoverage": true,
            "codeCoverageExclude": [
              "src/environments/**",
              "src/**/**.module.ts",
              "src/test.ts",
              "src/polyfills.ts",
              "src/**/search-results/**/*.ts",
              "src/**/search-bar/**/*.ts",
              "src/**/shared/confirm-dialog/**/*.ts",
              "src/**/shared/error-dialog/**/*.ts",
              "src/**/shared/markdown/**/*.ts",
              "src/**/services/cer-api.service.ts",
              "src/**/services/research-hub-api.service.ts",
              "src/**/directives/search-result-link.directive.ts",
              "src/**/pipes/list-item-to-router-link.pipe.ts"
            ],
            "karmaConfig": "karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "src/styles.scss"
            ],
            "assets": [
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.json",
              "tsconfig.spec.json"
            ],
            "exclude": []
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  },
  "defaultProject": "research-hub-web",
  "cli": {
    "analytics": false
  }
}