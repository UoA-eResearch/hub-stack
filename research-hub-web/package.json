{
  "name": "research-hub-web",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "dev": "concurrently \"npm run graphql-server-watch\" \"sleep 3 && npm run generate-watch\" \"sleep 3 && npm start\" \"sleep 3 && npm run test-watch\"",
    "start": "ng serve",
    "prestart": "npm run generate",
    "build": "ng build",
    "postinstall": "ngcc",
    "test": "ng test",
    "test-ci": "ng test --watch=false",
    "test-watch": "ng test --watch=true",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundle-report": "ng build --stats-json && webpack-bundle-analyzer www/stats.json",
    "e2e-ci": "ng e2e --webdriverUpdate=false",
    "testrunner": "./test.sh",
    "generate": "graphql-codegen",
    "generate-watch": "graphql-codegen --watch './src/app/graphql/queries/*.graphql'",
    "graphql-server": "nodemon ../cer-graphql/index.js",
    "graphql-server-watch": "npm run dev --prefix ../cer-graphql"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.0.9",
    "@angular/cdk": "10.1.3",
    "@angular/common": "10.0.9",
    "@angular/compiler": "10.0.9",
    "@angular/core": "10.0.9",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "10.0.9",
    "@angular/material": "10.1.3",
    "@angular/platform-browser": "10.0.9",
    "@angular/platform-browser-dynamic": "10.0.9",
    "@angular/router": "10.0.9",
    "@apollo/client": "^3.1.3",
    "@apollo/link-context": "^2.0.0-beta.3",
    "@contentful/rich-text-html-renderer": "^14.1.1",
    "@contentful/rich-text-types": "^14.1.1",
    "@uoa/auth": "^0.2.0",
    "@uoa/error-pages": "0.0.7",
    "apollo-angular": "^2.0.3",
    "apollo-angular-link-http": "^1.11.0",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-link": "^1.2.14",
    "apollo-link-error": "^1.1.13",
    "classlist.js": "^1.1.20150312",
    "contentful": "^7.14.6",
    "date-fns": "~2.14.0",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.10.4",
    "localforage": "^1.7.4",
    "marked": "~1.1.0",
    "ngx-take-until-destroy": "^5.4.0",
    "ngx-webstorage-service": "^4.1.0",
    "rxjs": "^6.6.2",
    "tslib": "^2.0.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.6",
    "@angular/cli": "^10.0.6",
    "@angular/compiler-cli": "^10.0.9",
    "@angular/language-service": "~10.0.9",
    "@graphql-codegen/cli": "^1.17.0",
    "@graphql-codegen/fragment-matcher": "^1.17.0",
    "@graphql-codegen/introspection": "^1.17.0",
    "@graphql-codegen/typescript": "^1.17.0",
    "@graphql-codegen/typescript-apollo-angular": "^1.17.0",
    "@graphql-codegen/typescript-operations": "^1.17.0",
    "@graphql-codegen/typescript-resolvers": "^1.17.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "aws-sdk": "^2.760.0",
    "aws4": "^1.10.1",
    "browserstack-local": "^1.4.5",
    "change-case": "^4.1.1",
    "codelyzer": "^6.0.0",
    "concurrently": "^5.2.0",
