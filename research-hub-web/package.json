    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-verbose-reporter": "0.0.6",
    "nodemon": "^2.0.4",
    "protractor": "^7.0.0",
    "sass": "^1.26.9",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.7",
    "webdriver-manager": "^12.1.7"
  },
  "browser": {
    "crypto": false
  }
