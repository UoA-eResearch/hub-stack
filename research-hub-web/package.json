{
  "name": "research-hub-web",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "dev": "concurrently \"npm run graphql-server-watch\" \"sleep 3 && npm run generate-watch\" \"sleep 3 && npm start\" \"sleep 3 && npm run test-watch\"",
    "dev-no-gen": "concurrently \"sleep 3 && npm run generate-watch\" \"sleep 3 && ng serve\" \"sleep 3 && npm run test-watch\"",
    "qdev": "concurrently \"npm run graphql-server-watch\" \"sleep 3 && npm run generate-watch\" \"sleep 3 && npm start\"",
    "qdev-no-gen": "concurrently \"npm run graphql-server-watch\" \"sleep 3 && ng serve\"",
    "start": "ng serve",
    "prestart": "npm run generate",
    "build": "ng build --source-map=false",
    "prebuild": "npm run generate",
    "postinstall": "ngcc",
    "test": "ng test",
    "test-ci": "ng test --watch=false --source-map=false",
    "test-watch": "ng test --watch=true",
    "lint": "ng lint",
    "bundle-report": "ng build --stats-json && webpack-bundle-analyzer www/stats.json",
    "testrunner": "./test.sh",
    "generate": "graphql-codegen",
    "generate-watch": "graphql-codegen --watch './src/app/graphql/queries/*.graphql'",
    "graphql-server": "nodemon ../cer-graphql/index.js",
    "graphql-server-watch": "npm run dev --prefix ../cer-graphql",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "e2e": "cypress run -q --config record=false,pageLoadTimeout=10000,defaultCommandTimeout=10000",
    "e2e-gui": "npm run cy:open",
    "e2e-ci": "concurrently \"npm run start -- -c ${BRANCH_NAME}\" \"wait-on http-get://localhost:4200 && npm run e2e\" --kill-others --success first",
    "e2e-serve": "concurrently \"npm run qdev\" \"wait-on http-get://localhost:4200 && npm run e2e\" --kill-others --success first",
    "e2e-serve-gui": "concurrently \"npm run qdev\" \"wait-on http-get://localhost:4200 && npm run cy:open\" --kill-others --success first"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.0.3",
    "@angular/cdk": "^11.0.2",
    "@angular/common": "11.0.3",
    "@angular/compiler": "11.0.3",
    "@angular/core": "11.0.3",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "11.0.3",
    "@angular/material": "^11.0.2",
    "@angular/platform-browser": "11.0.3",
    "@angular/platform-browser-dynamic": "11.0.3",
    "@angular/router": "11.0.3",
    "@apollo/client": "^3.1.3",
    "@apollo/link-context": "^2.0.0-beta.3",
    "@contentful/rich-text-html-renderer": "^14.1.1",
    "@contentful/rich-text-types": "^14.1.1",
    "@uoa/auth": "^0.4.0",
    "@uoa/error-pages": "0.0.7",
    "apollo-angular": "^2.0.3",
    "apollo-angular-link-http": "^1.11.0",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-link": "^1.2.14",
    "apollo-link-error": "^1.1.13",
    "classlist.js": "^1.1.20150312",
    "contentful": "^7.14.6",
    "date-fns": "~2.14.0",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.10.4",
    "jasmine": "^2.99.0",
    "localforage": "^1.7.4",
    "markdown-to-html-pipe": "^1.2.5",
    "marked": "~1.1.0",
    "ngx-contentful-rich-text": "^0.4.0",
    "ngx-device-detector": "^2.0.6",
    "ngx-pagination": "^5.0.0",
    "ngx-skeleton-loader": "^2.9.1",
    "ngx-take-until-destroy": "^5.4.0",
    "ngx-webstorage-service": "^4.1.0",
    "rxjs": "^6.6.2",
    "smoothscroll-polyfill": "^0.4.4",
    "tslib": "^2.0.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.3",
    "@angular/cli": "^11.1.1",
    "@angular/compiler-cli": "11.0.3",
    "@angular/language-service": "~11.0.3",
    "@graphql-codegen/cli": "^1.17.0",
    "@graphql-codegen/fragment-matcher": "^1.17.0",
    "@graphql-codegen/introspection": "^1.17.0",
    "@graphql-codegen/typescript": "^1.17.0",
    "@graphql-codegen/typescript-apollo-angular": "^1.17.0",
    "@graphql-codegen/typescript-operations": "^1.17.0",
    "@graphql-codegen/typescript-resolvers": "^1.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "aws-sdk": "^2.760.0",
    "aws4": "^1.10.1",
    "browserstack-local": "^1.4.5",
    "change-case": "^4.1.1",
    "codelyzer": "^6.0.0",
    "concurrently": "^5.2.0",
    "cypress": "^7.1.0",
    "fibers": "^5.0.0",
    "jasmine-core": "^2.99.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-spec-reporter": "0.0.32",
    "karma-verbose-reporter": "0.0.6",
    "nodemon": "^2.0.4",
    "sass": "^1.26.9",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5",
    "wait-on": "^5.2.1",
    "webdriver-manager": "^12.1.7"
  },
  "browser": {
    "crypto": false
  }
}
