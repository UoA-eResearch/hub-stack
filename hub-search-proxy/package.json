    "deploy": "sls deploy",
    "cred-np": "python C:\\opt\\uoa_project\\utility-aws-cli-access\\aws_saml_login.py --idp iam.auckland.ac.nz --role 518380838815 --account devops --profile uoa-nonprod --user etan221",
